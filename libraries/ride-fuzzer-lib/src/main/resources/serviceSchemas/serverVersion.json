{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title" : "target fuzzer server version",
    "type": "object",
    "description": "this is the schema the response in a version check",
	"required": ["projectVersion"],
	"additionalProperties": true,
	"properties": {
		"projectName": { "type": "string" },
		"projectVersion": { "type": "string" },
